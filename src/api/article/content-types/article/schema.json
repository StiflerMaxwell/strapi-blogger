{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "author": {
      "type": "string"
    },
    "content": {
      "type": "blocks"
    },
    "custorm_content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "cover_image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "view_count": {
      "type": "integer"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "website": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::website.website"
    },
    "reading_records": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::reading-record.reading-record",
      "mappedBy": "article"
    },
    "pin_order": {
      "type": "integer"
    },
    "seo": {
      "type": "json"
    }
  }
}
