{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "blocks"
    },
    "cover_image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "view_count": {
      "type": "integer"
    },
    "website": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::website.website",
      "inversedBy": "article"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    }
  }
}
